<!DOCTYPE html>
<html>
  <head>
    <title>Pushjaw</title>
    <%= stylesheet_link_tag    "style", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>

    <link href="http://vjs.zencdn.net/c/video-js.css" rel="stylesheet">


    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="HandheldFriendly" content="true">

    <link rel="shortcut icon" href="/assets/favicon.ico">
    <link rel="shortcut icon" href="/assets/favicon.gif">
    <link rel="icon" href="/assets/images/favicon.ico">
    <!--[if IE]><link rel="stylesheet" type="text/css" href="/assets/style.css" /><![endif]-->
    <%#*<link rel="stylesheet" type="text/css" href="/assets/jquery.mCustomScrollbar.css">%>
    <link rel="icon" href="/assets/favicon.ico">

  </head>	

  <body onload="#home">

    <%= yield %>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
    <script src="http://vjs.zencdn.net/c/video.js"></script>

    <script type="text/javascript" src="/assets/jquery.poshytip.js"></script>
    <script type="text/javascript" src="/assets/slides.min.jquery.js"></script>
    <script type="text/javascript" src="/assets/imgpreload.js"></script>
    <script type="text/javascript" src="/assets/parallax.js"></script>
    <script type="text/javascript" src="/assets/easing.js"></script>
    <script type="text/javascript" src="/assets/jcontent.js"></script>
    <script type="text/javascript" src="/assets/tabs.js"></script>

    <script type="text/javascript" src="/assets/jquery.clouds.js"></script>
<%#*<script type="text/javascript" src="/assets/jquery.mCustomScrollbar.js"></script>%>
    <script type="text/javascript" src="/assets/jquery.mousewheel.min.js"></script>
    <script type="text/javascript" src="/assets/jquery.fancybox-1.3.4.pack.js"></script>

    <script>!window.jQuery.ui && document.write(unescape('%3Cscript src="/assets/jquery-ui-1.8.21.custom.min.js"%3E%3C/script%3E'))</script>

    <script type="text/javascript">
      $("document").ready(function(){$("div#sliderwraper").jContent({orientation: 'horizontal', easing: "easeOutCirc", duration: 500,auto: true});});

      $('.cloudmoving').clouds({xmlMap: '/assets/clouds.xml',bobHeight: 2,speed: 12});      

      <%#*$(".content_6").mCustomScrollbar({%>
        <%#*horizontalScroll:true,%>
        <%#*advanced:{%>
          <%#*autoExpandHorizontalScroll:true%>
        <%#*}%>
      <%#*});%>
    </script> 

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBOXOBmlC8dhsb4hNqiCJJQe-orUQvRrbo&sensor=true">
    </script>

    <script type="text/javascript">
      function load_home() {
        $("document").ready(function(){
          window.location.href = "#home";
        })
      }
      window.onload = load_home;
      
    </script>
    <%= yield :javascript %>

  </body>
</html>
